#ifndef DOG_STRUCT_H
#define DOG_STRUCT_H

/**
 * struct dog - Defines a dog structure.
 * @name: Pointer to a character string for the dog's name.
 * @age: The age of the dog as a float.
 * @owner: Pointer to a character string for the owner's name.
 */
struct dog
{
	char *name;
	float age;
	char *owner;
};

#endif /* DOG_STRUCT_H */
